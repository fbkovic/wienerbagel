{
    "version": 2,
    "name": "wienerbagel-webseite",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": null,
    "outputDirectory": "dist/public",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/server"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "functions": {
        "api/server.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}